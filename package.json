{
  "name": "websites",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.11",
    "eslint-config-custom": "workspace:*",
    "postcss": "^8.4.16",
    "prettier": "latest",
    "prettier-plugin-tailwindcss": "^0.2.0",
    "tailwindcss": "^3.1.8",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.18.1"
}
